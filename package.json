{
  "name": "dahaeng-main-web",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier . --write --list-different",
    "check:all": "pnpm check:lint && pnpm check:prettier && pnpm check:type",
    "check:lint": "turbo check:lint",
    "check:prettier": "prettier . --check || exit 1",
    "check:type": "turbo check:type"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.8.3",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
